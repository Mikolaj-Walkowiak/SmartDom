using sysbus

mach create
machine LoadPlatformDescription @platforms/cpus/stm32f103.repl

$bin?=@build/zephyr/zephyr.elf

showAnalyzer uart2


macro start
"""
    sysbus LoadELF $bin
    emulation CreateEnvironment "env"
    runMacro $reset
"""

macro reset
"""
    env Humidity 100
"""


runMacro $start
